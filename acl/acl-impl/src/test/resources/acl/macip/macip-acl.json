{
  "access-lists": {
    "acl": [
      {
        "acl-name": "macip-acl",
        "acl-type": "vpp-acl:vpp-macip-acl",
        "tag": "macip-tag-value",
        "access-list-entries": {
          "ace": [
            {
              "rule-name": "macip-rule",
              "matches": {
                "vpp-macip-ace-nodes": {
                  "source-ipv4-network": "192.168.2.2/32",
                  "source-mac-address": "aa:aa:aa:aa:aa:aa",
                  "source-mac-address-mask": "ff:00:00:00:00:00"
                }
              },
              "actions": {
                "permit": {}
              }
            }
          ]
        }
      }
    ]
  }
}