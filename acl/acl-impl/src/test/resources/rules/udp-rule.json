{
  "acls": {
    "acl": [
      {
        "name": "standard-acl",
        "type": "vpp-acl:vpp-acl",
        "aces": {
          "ace": [
            {
              "name": "imcp-rule",
              "matches": {
                "ipv4": {
                  "destination-ipv4-network": "192.168.2.1/32",
                  "source-ipv4-network": "192.168.2.2/32"
                },
                "udp": {
                  "source-port": {
                    "lower-port": "1",
                    "upper-port": "5487"
                  },
                  "destination-port": {
                    "lower-port": "87",
                    "upper-port": "6745"
                  }
                }
              }
            }
          ]
        }
      }
    ]
  }
}