{
  "acls": {
    "acl": [
      {
        "name": "standard-acl",
        "type": "vpp-acl:vpp-acl",
        "aces": {
          "ace": [
            {
              "name": "no-protocol-rule",
              "matches": {
                "ipv4": {
                  "source-ipv4-network": "192.168.2.2/32",
                  "destination-ipv4-network": "192.168.2.1/32"
                }
              }
            }
          ]
        }
      }
    ]
  }
}